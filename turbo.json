{
  "globalEnv": [
    "HTTP_BACKEND",
    "WS_URL",
    "PRESENCE_URL"
  ],
  "$schema": "https://v2-8-1.turborepo.dev/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [
        "apps/Frontend/.next/**"
      ]
    },
    "dev": {
      "dependsOn": ["^dev", "^build"],
      "cache": false,
      "persistent": false
    }
  }
}
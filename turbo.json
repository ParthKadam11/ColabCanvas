{
  "globalEnv": [
    "HTTP_BACKEND",
    "WS_URL",
    "PRESENCE_URL"
  ],
  "$schema": "https://v2-8-1.turborepo.dev/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [
        "apps/Frontend/.next/**"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": false
    }
  }
}
